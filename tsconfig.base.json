{
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node", "jest"],
    "rootDir": ".",
    "lib": ["esnext"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "removeComments": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "target": "es5"
  }
}

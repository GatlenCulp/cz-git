{
  "name": "docs",
  "private": true,
  "scripts": {
    "build": "pnpm build:init && vitepress build --clean-cache && pnpm build:post",
    "build:init": "esno .vitepress/build/scripts/prebuild.ts",
    "build:post": "esno .vitepress/build/scripts/postbuild.ts",
    "dev": "vitepress dev --clean-cache --host --port 8080",
    "serve": "vitepress serve --clean-cache --host --port 8000"
  },
  "dependencies": {
    "@vueuse/core": "^9.1.1",
    "shiki": "^0.11.1",
    "vue": "^3.2.38"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.7",
    "@unocss/reset": "^0.45.14",
    "@vitejs/plugin-vue": "^3.0.3",
    "markdown-it-container": "^3.0.0",
    "markdown-it-mark": "^3.0.1",
    "medium-zoom": "^1.0.6",
    "sitemap": "^7.1.1",
    "unocss": "^0.45.14",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.9",
    "vite-plugin-pwa": "^0.12.4",
    "vite-plugin-radar": "^0.5.0",
    "vitepress": "1.0.0-alpha.13"
  }
}

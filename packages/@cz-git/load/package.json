{
  "name": "@cz-git/load",
  "version": "1.1.1",
  "description": "commitizen and commitlint config loader for cz-git",
  "keywords": [
    "cz-git",
    "commitizen",
    "commitlint",
    "commitizen-config-loader",
    "commitlint-config-loader"
  ],
  "homepage": "https://github.com/Zhengqbbb/cz-git/tree/main/packages/load#readme",
  "bugs": {
    "url": "https://github.com/Zhengqbbb/cz-git/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zhengqbbb/cz-git.git"
  },
  "license": "MIT",
  "author": "Zhengqbbb <1074059947@qq.com>",
  "main": "lib/load.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rimraf *.tsbuildinfo lib && tsc -b",
    "dev": "tsc -b --watch",
    "lint": "eslint --ext .ts",
    "release": "standard-version --release-as patch",
    "release-as": "standard-version --release-as",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "package.json": "sort-package-json"
  },
  "publishConfig": {
    "access": "public"
  }
}

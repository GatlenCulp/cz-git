{
  "name": "@cz-git/loader",
  "version": "1.2.9",
  "description": "commitizen and commitlint configure loader for cz-git",
  "keywords": [
    "cz-git",
    "commitizen-loader",
    "commitlint-loader"
  ],
  "homepage": "https://github.com/Zhengqbbb/cz-git/tree/main/packages/%40cz-git/plugin-loader",
  "bugs": {
    "url": "https://github.com/Zhengqbbb/cz-git/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zhengqbbb/cz-git.git",
    "directory": "packages/@cz-git/plugin-loader"
  },
  "license": "MIT",
  "author": "Zhengqbbb <1074059947@qq.com>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "pnpm clean && tsc -b tsconfig.build.json && tsup && rimraf dist",
    "build:dev": "pnpm clean && tsc -b",
    "clean": "rimraf *.tsbuildinfo lib dist"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "devDependencies": {
    "@commitlint/resolve-extends": "^16.2.1",
    "@commitlint/types": "^16.2.1",
    "cosmiconfig": "^7.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
